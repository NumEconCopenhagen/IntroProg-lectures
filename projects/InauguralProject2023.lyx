\end_inset


\end_layout

\begin_layout Standard
In 
\begin_inset CommandInset href
LatexCommand href
name "Siminski and Yetsenga (2022)"
target "https://drive.google.com/file/d/1neoRn4vScoHAIVAzPAdraqyf-RhuEkP7/"
literal "false"

\end_inset

, 
\emph on
